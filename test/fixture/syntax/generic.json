        "syntax": {
            "generic": true,
            "properties": {
                "test": "test( <declaration-value> )"
            }
        },
        "skipped": {
            "comment": "such values can't be parsed currently",
            "valid": [
                "test( foo(!) )",
                "test( foo(;) )",
                "test( ([{}]) )"
            ]
        },
        "valid": [
            "test( foo )",
            "test( 123px )"
        ],
        "invalid": [
            "test(  )",
            "test( foo 'asd\n )",
            "test( foo url(( )",
            "test( foo!important )",
            "test( foo;123 )",
            "test( ) )",
            "test( ] )",
            "test( } )",
            "test( (]) )",
            "test( (}) )",
            "test( [)] )"
        ]
    },
